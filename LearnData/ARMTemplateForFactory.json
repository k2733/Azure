		"Azure_credential": {
			"type": "secureString",
			"metadata": "“Azure”的“credential”的安全字符串"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "“AzureStorageLinkedService”的“connectionString”的安全字符串"
		},
		"VCC_connectionString": {
			"type": "secureString",
			"metadata": "“VCC”的“connectionString”的安全字符串"
		},
		"Azure_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "k2733"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CopyPipeline_k5f')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_k5f",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "OracleSource",
								"oracleReaderQuery": "SELECT \r\n     LOGICAL_TABLE_NUMBER 固定値番号\r\n    ,LOGICAL_TABLE_NAME   固定値名称\r\n    ,KEY_ITEM_01          給与グループ\r\n    ,KEY_ITEM_02          従業員区分\r\n    ,KEY_ITEM_03          分類\r\n    ,FIXED_ITEM_01        公開日\r\nFROM \r\n    ZHR_CM_FIXED_VALUE_TB FIXED \r\nWHERE \r\n  LOGICAL_TABLE_NUMBER = '802'\r\n  AND KEY_ITEM_03 = '給与'\r\nAND SYSDATE BETWEEN EFFECTIVE_START_DATE AND EFFECTIVE_END_DATE\r\nORDER BY LOGICAL_TABLE_NUMBER,KEY_ITEM_01,KEY_ITEM_02;",
								"partitionOption": "None"
							},
							"sink": {
								"type": "DelimitedTextSink",
								"storeSettings": {
									"type": "AzureBlobStorageWriteSettings"
								},
								"formatSettings": {
									"type": "DelimitedTextWriteSettings",
									"quoteAllText": true,
									"fileExtension": ".txt"
								}
							},
							"enableStaging": false,
							"validateDataConsistency": false
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_k5f",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_k5f",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SourceDataset_k5f')]",
				"[concat(variables('factoryId'), '/datasets/DestinationDataset_k5f')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Simulation process')]",
