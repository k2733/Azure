{
	"name": "Simulation process",
	"properties": {
		"activities": [
			{
				"name": "S2",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "SELECT \n     LOGICAL_TABLE_NUMBER 固定値番号\n    ,LOGICAL_TABLE_NAME   固定値名称\n    ,KEY_ITEM_01          給与グループ\n    ,KEY_ITEM_02          従業員区分\n    ,KEY_ITEM_03          分類\n    ,FIXED_ITEM_01        公開日\nFROM \n    ZHR_CM_FIXED_VALUE_TB FIXED \nWHERE \n  LOGICAL_TABLE_NUMBER = '802'\n  AND KEY_ITEM_03 = '給与'\nAND SYSDATE BETWEEN EFFECTIVE_START_DATE AND EFFECTIVE_END_DATE\nORDER BY LOGICAL_TABLE_NUMBER,KEY_ITEM_01,KEY_ITEM_02",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Info",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "AzureStorageLinkedService",
								"type": "LinkedServiceReference"
							},
							"path": "spfile"
						}
					},
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "OracleSource2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "S2Bolb",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "S2",
						"dependencyConditions": [
							"Succeeded",
							"Failed",
							"Completed",
							"Skipped"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "SELECT '@{pipeline（）.DataFactory}' 作为DataFactory_Name，\n\n'@{pipeline（）.Pipeline}' 作为Pipeline_Name，\n\n'@{activity（'copytables'）.output.executionDetails[0].source.type}' 作为Source_Type，\n\n'@{activity（'copytables'）.output.executionDetails[0].sink.type}' 作为Sink_Type，\n\n'@{activity（'copytables'）.output.executionDetails[0].status}' 作为Execution_Status，\n\n'@{activity（'copytables'）.output.rowsRead}' as RowsRead，\n\n'@{activity（'copytables'）.output.rowsCopied}' as RowsWritten\n\n'@{activity（'copytables'）.output.copyDuration}' as CopyDurationInSecs，\n\n'@{activity（'copytables'）.output.executionDetails[0].start}' 作为CopyActivity_Start_Time，\n\n'@{utcnow（）}' 作为CopyActivity_End_Time，\n\n'@{pipeline（）.RunId}' as RunId，\n\n'@{pipeline（）.TriggerType}' as TriggerType，\n\n'@{pipeline（）.TriggerName}' as TriggerName，\n\n'@{pipeline（）.TriggerTime}' as TriggerTime",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "OracleSource1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}