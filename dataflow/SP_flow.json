{
	"name": "SP_flow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "FIXED_VALUE_Blob",
						"type": "DatasetReference"
					},
					"name": "FIXED"
				},
				{
					"dataset": {
						"referenceName": "KYUUYOSHOUYO_Blob",
						"type": "DatasetReference"
					},
					"name": "KYUUYOSHOUYO"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> FIXED",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> KYUUYOSHOUYO",
				"FIXED, KYUUYOSHOUYO join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}