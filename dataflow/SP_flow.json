{
	"name": "SP_flow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "FIXED_VALUE_Blob",
						"type": "DatasetReference"
					},
					"name": "FIXED"
				},
				{
					"dataset": {
						"referenceName": "KYUUYOSHOUYO_Blob",
						"type": "DatasetReference"
					},
					"name": "KYUUYOSHOUYO"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Result",
						"type": "DatasetReference"
					},
					"name": "Result"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> FIXED",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> KYUUYOSHOUYO",
				"FIXED sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Result"
			]
		}
	}
}